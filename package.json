{
  "name": "msg_technical_assessment",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "tests": "codeceptjs run --steps",
    "generate:report": "allure generate output --clean -o allure-report",
    "test:web": "codeceptjs run --grep @web --plugins allure --steps",
    "test:web-with-report": "npm run test:web && npm run report",
    "test:api": "codeceptjs run --grep @api --steps",
    "test:features": "codeceptjs run --features",
    "test:headless": "HEADLESS=true codeceptjs run --features --plugins allure --steps",
    "clean": "rm -rf output/*"
  },
  "keywords": [
    "automation",
    "codeceptjs",
    "playwright",
    "typescript",
    "gherkin",
    "rest"
  ],
  "author": "John Angelo Lobramonte",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@codeceptjs/allure-legacy": "^1.0.2",
    "@codeceptjs/configure": "^1.0.6",
    "@types/chai": "^5.2.2",
    "@types/node": "^24.3.0",
    "allure-codeceptjs": "^3.3.3",
    "allure-commandline": "^2.34.1",
    "chai": "^4.5.0",
    "codeceptjs": "^3.7.4",
    "playwright": "^1.55.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
